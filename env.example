VITE_SUPABASE_URL=
VITE_SUPABASE_PUBLISHABLE_KEY=

# Gemini (Google AI Studio)
VITE_GEMINI_API_KEY=
# Opcional
VITE_GEMINI_MODEL=gemini-1.5-flash

# Backend Python opcional (faster-whisper) para refinar a transcrição
# Ex: http://localhost:8008
VITE_LOCAL_TRANSCRIBE_URL=

# Backend Python opcional (streaming via WebSocket) para transcrição ao vivo
# Ex: ws://localhost:8008
VITE_LOCAL_TRANSCRIBE_WS=

# Backend Python opcional (Vosk streaming de baixa latência, recomendado para "tempo real")
# Ex: ws://localhost:8008
VITE_LOCAL_VOSK_WS=

# Aumenta a sensibilidade (ganho) do microfone no medidor e no STT local (Vosk).
# 1 = normal | 2-4 = mais sensível (recomendado). Máx 6.
VITE_MIC_GAIN=2

# Backend: Vosk (acurácia x tamanho)
# large = baixa latência com melhor acurácia (download grande) | small = download menor
# (se não setar, default = large)
# VOSK_MODEL_SIZE=large

